{
    "version": 2,
    "env": {
      "NODE_ENV": "production",
      "PORT": 4000,
      "API_URL_rickandmortyapi": "https://rickandmortyapi.com/api/"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ]
  }