{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**/*"],
                "tsconfig": "./tsconfig.json"
            }
        }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ]
  }